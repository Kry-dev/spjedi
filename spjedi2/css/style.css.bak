
body { background:#fff; font-family: 'Trebuchet MS' !important;}

ul { margin-bottom: 0;}
a:hover { text-decoration: none;}
li { list-style: none;}

.site_header { height: 85px; width: 1140px; margin: 0 auto; padding: 0 0px; background-color: #fff; }
.site_header .logo_left { width: 30px; display: inline-block; vertical-align: middle;}
.site_header .site_logo { width: 20%; display: inline-block; margin-top: 10px; vertical-align: middle;}
.site_header .site_logo a { width: 170px; display: block;}
.site_header .site_menu { display: inline-block; text-align: right; width: 79%; padding: 10px 0; vertical-align: middle;}
.site_header .logo_right { display: inline-block; width: 40px; vertical-align: middle;}
.site_header img { width: 100%;}

.site_header .site_menu ul { padding-left: 0;}
.site_header .site_menu ul li { display: inline-block;}
.site_header .site_menu ul li a { font-size: 18px; color: #324754; font-family: 'Trebuchet MS'; padding: 10px 20px; }
.site_header .site_menu ul li a:hover { color: #69C8C7; text-decoration: none;}

.slider { width: 100%; margin: 0 auto;}
.slider .site_slider img { width: 100%;}
.slider { border-bottom: 5px solid #101B2D;border-top: 5px solid #101B2D; display: inline-block;}

.explore_process { padding: 20px 0; background-color:rgba(0, 0, 0, 0.48)}
.explore_process .force_process { width: 49%; display: inline-block;}
.explore_process .force_process img { width: 100%;}
.explore_process .heading_process { width: 49%; display: inline-block;}
.explore_process .heading_process .heading_bg { width: 350px; margin: 0 auto; background-color: #D1D2D4; border-radius: 4px; text-align: center; padding: 5px 0;}
.explore_process .heading_process .heading_bg .explore_text { display: inline-block; text-align: center;width: 112px;  vertical-align: middle;}
.explore_process .heading_process .heading_bg .process_icon { display: inline-block; width: 60px;  vertical-align: middle;  margin: 0 5px;}
.explore_process .heading_process .heading_bg .seeour_proce_text { display: inline-block;  vertical-align: middle;}
.explore_process .heading_process .heading_bg .process_icon img { width: 100%;}
.explore_process .heading_process .heading_bg span { font-size: 14px; color: #324754;}

.our_process_box { padding-left: 20%; padding-right: 20%; background-color:rgba(0, 0, 0, 0.48); margin: 0 auto; text-align: center;}
.our_process_box .assess_mess { width: 340px; display: inline-block; min-height: 151px; background-color: #D1D2D4; border:2px solid #454D59; 
	border-radius: 10px; margin: 15px; vertical-align: top; text-align: left; padding: 15px;}
.our_process_box .assess_mess h5 { color: #013333; font-size: 22px; margin-top: 0;}
.our_process_box .assess_mess .process_box_text { padding:10px; border:1px solid #7F7E80; font-size: 18px; color: #231F20;}
.our_process_box .assess_mess .process_box_text span { display: block; margin-bottom: 5px;}
.our_process_box .assess_mess .process_box_text a span { color: #BE1E2D;}
.our_process_box .assess_mess .process_box_text a:hover span { color: #aa0a19;}

.how_3box { margin: 0 auto; text-align: center; width: 100%; background-color:rgba(0, 0, 0, 0.48)}
.how_3box .apart_box { width: 340px; margin: 15px; display: inline-block; vertical-align: top; color: white;}
.how_3box .apart_box h3 { font-size: 28px; color: #231F20;}
.how_3box .apart_box .list_3bock { background-color: #101B2D; border-radius: 10px;  padding: 10px;}
.how_3box .apart_box .how_do_bock {  height: 340px; overflow: hidden;}
.how_3box .apart_box .how_do_bock span { color: #00AEEF; font-size: 18px; }
.how_3box .apart_box .how_do_bock p { font-size: 14px; color: #E6E7E8;  margin-top: 8px;}
.how_3box .apart_box .how_do_bock .border { height: 1px; width: 80px; background-color: #fff; display: inline-block; width: 105px; margin-top: 20px;
    vertical-align: top;}
.how_3box .apart_box .how_do_bock .bock_icon { display: inline-block; background-color: #101B2D; position: relative; z-index: 1;}
.how_3box .apart_box .how_do_bock .full_border_bock { border:1px solid #fff; border-bottom: 0 none; padding: 50px 5px; position: relative; top: -35px; height: 100%;}
.bock_icon > img {  width: 80%;}

.full_width_follow { background-color: rgba(0, 0, 0, 0.48); margin: 0 auto; width: 100%; text-align: center; padding: 30px 0;}
.full_width_follow h2 { font-size: 32px; padding-left:25%; padding-right:25%; margin-top: 0; margin-bottom: 20px; }
.full_width_follow span { font-size: 18px;}

footer { margin: 0 auto; width: 100%; background-color: #10172C; color: #E6E7E8;padding: 30px 0;}
.about_contact_footer .footer_about { width: 50%; display: inline-block;}
.about_contact_footer .footer_contact { width: 45%; display: inline-block; vertical-align: top; float: right;}
.about_contact_footer .footer_about .copyright { margin-top: 20px;}
.about_contact_footer { width: 1140px; margin:0 auto; margin-top: 30px; overflow: hidden;}
.footer_contact span { color: #E6E7E8; display: block; vertical-align: top; font-size: 14px; margin-bottom: 5px;}
.footer_contact span span { display: inline-block;}
.footer_contact span a { color: #E6E7E8; font-size: 14px;}
.footer_contact span a:hover { color:#00aeef; }
.footer_contact p { color: #E6E7E8; font-size: 14px;}
.footer_contact .contact_addr .social_icon li { display: inline-block; margin:0 8px;}
.footer_contact .contact_addr .social_icon { display: inline-block;}
.footer_contact span.addr_list { display: inline-block;  margin-top: 10px;}
.footer_contact .contact_addr .social_icon li img { width: 100%;}
.footer_contact .contact_addr .social_icon li a { width: 40px; display: block;}

#menu-button, .copyright_center { display: none;}

/* about us */
.about_page .about_cont ul { padding-left: 0;}
.about_page .about_cont {  margin: 0 auto; text-align: center; width: 1140px;}
.about_page .about_cont li { display: inline-block; margin: 15px; min-height: 151px; padding: 15px; vertical-align: top; width: 340px;}
.about_page .about_cont li .about_img img { border:4px solid #fff; border-radius: 50%;}
.about_page .about_cont li .about_detail { background-color: #D1D2D4; border-radius: 10px; margin-top: 20px; padding: 3px;  min-height: 257px;}
.about_page .about_cont li .about_detail h5 { color: #1C75BC; font-size: 18px; font-weight: bold; padding-bottom: 5px;border-bottom: 2px solid #BE1E2D; display: inline-block;}
.about_page .about_cont li .about_detail span { color: #324754; font-size: 16px; margin-bottom: 8px; display: block;}
.about_page .about_cont li .about_detail strong { color: #324754; font-size:16px;font-weight: bold; display: block; }
.about_page .about_cont li .about_detail p { margin:0 auto; color: #324754; font-size: 16px;}
.about_page .about_cont li.about_2 .about_detail h5 { color: #BE1E2D;}
.about_page .about_cont li.about_3 .about_detail h5 { color: #009444;}
.about_page .about_cont li.about_4 .about_detail h5 { color: #8B5E3C;}
.about_page .about_cont li.about_5 .about_detail h5 { color: #662D91;}
.about_page .about_cont li.about_6 .about_detail h5 { color: #DA1C5C;}
.about_services .services_head { text-align: right;}
.about_services .all_services ul { padding-left:0;}
.about_services .all_services li { display: inline-block; margin: 8px; vertical-align: top; width: 166px; text-align: center;}
.about_services .all_services li .service_detail { background-color: #D1D2D4; border-radius:8px; padding:8px; min-height: 270px; margin-top: 20px;}
.about_services .all_services li .service_detail h4 { color: #324754; font-size: 14px; font-weight: bold; display: inline-block; border-bottom: 2px solid #BE1E2D; padding-bottom: 5px;}
.about_services .all_services li .service_detail p { color: #324754; font-size: 14px;}
.about_services .all_services { width: 1140px; margin: 20px auto 0; text-align: center;}

footer.about_footer { background-color: #314754; display: inline-block;  margin-top: 20px;}
footer.about_footer .site_footer { width: 1140px; margin: 0 auto; overflow: hidden;}
footer.about_footer .footer_cont { float: left;}
footer.about_footer .addr_social { float: right;}
footer.about_footer .footer_cont span { color: #E6E7E8; font-size: 14px;}
footer.about_footer .footer_cont .cont_addr span { display: block;}
footer.about_footer .footer_cont .cont_addr span span { display: inline-block;}
footer.about_footer .addr_social .site_footer_logo li { display: inline-block; margin: 0 5px;}
footer.about_footer .addr_social .footer_email_social .email_footer { display: inline-block;}
footer.about_footer .addr_social .footer_email_social .social_icon { display: inline-block;}
footer.about_footer .addr_social .footer_email_social .social_icon li { display: inline-block; margin: 0 5px;}
footer.about_footer .addr_social .site_footer_logo { text-align: right;}
footer.about_footer .addr_social .footer_email_social .social_icon li img { width: 100%;}
footer.about_footer .addr_social .footer_email_social .social_icon li a { width: 75%; display: inline-block;}
footer.about_footer .addr_social .footer_email_social .email_footer a { color: #fff;}

/* new home  */
