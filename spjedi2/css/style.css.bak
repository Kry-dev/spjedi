/* slider */
.slider_center { text-align: center; width: 100%; background: none; margin: 0 auto !important; left: 0 !important}
.responsive-slider .slides ul li { width: 100%;}
.slider_center h3 { white-space: normal; width: 50%; margin: 0 auto; font-size: 35px; font-family: "Trebuchet MS"; line-height: 45px;}
.slider_center.slider_btn a { font-size: 23px; color: #fff; background: #1e9791; padding:10px 55px; border-radius: 20px; font-family: "Trebuchet MS";font-weight: normal;}
.slider_center.slider_btn { width: 100%; padding-top:250px;}
.slider_center.slider_btn a:hover { background: #107c76; text-decoration: none;}
.slider_width h2 { white-space: normal; width: 50%; color: #fff; font-size: 34px;}
.slider_width { width: 1140px; background: none; text-align: left;}
.slider_width .slider_text { white-space: normal; width: 80%; font-size: 16px; margin: 20px auto}
.responsive-slider .slides ul li .slide-body .slider_width .caption.sub { bottom: -140px; text-align: left;}
